<div class="wrapper">
    <%var profile = JSON.parse(localStorage.getItem('profile'));%>
    <label for="store_name" class="required">Store Name</label>
    <input name="store_name" id="store_name" type="text" value="<%=profile.name%>" placeholder="Northwest Taxi">
    <label for="cashaddress" class="required">Cash Address</label>
    <input name="cashaddress" id="cashaddress" type="text" value="<%=profile.cashaddress%>" placeholder="bitcoincash:qq..">
    <label for="slpaddress">SLP Address</label>
    <input name="slpaddress" id="slpaddress" type="text" value="<%=profile.slpaddress%>" placeholder="simpleledger:qq..">
    <label for="fiat_currency">Currency</label>
        <select name="fiat_currency" id="fiat_currency">
            <option value="usd">USD</OPTION>
            <option value="aed">AED</OPTION>
            <option value="ars">ARS</OPTION>
            <option value="aud">AUD</OPTION>
